{
  "name": "podcast-resources",
  "version": "0.1.0",
  "bin": {
    "podcast-resources": "bin/podcast-resources.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lambda:ci": "npm ci --prefix lambda",
    "prettier": "prettier --write \"**/*.ts\"",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.70.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "1.70.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.2",
    "parcel": "2.0.0-beta.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.70.0",
    "@aws-cdk/aws-events": "1.70.0",
    "@aws-cdk/aws-events-targets": "1.70.0",
    "@aws-cdk/aws-lambda": "1.70.0",
    "@aws-cdk/aws-lambda-nodejs": "1.70.0",
    "@aws-cdk/aws-logs": "1.70.0",
    "@aws-cdk/aws-s3": "1.70.0",
    "@aws-cdk/aws-sqs": "^1.70.0",
    "@aws-cdk/core": "1.70.0",
    "source-map-support": "^0.5.16"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier"
    ]
  }
}
